package com.bank;

public class LoanProcess {
	
	//methods
	//method overloading //compiletime polymorphism  //static binding
	void loan(int loanamt, int gst, int pfee)
	{
		System.out.println("personal loan amount:"+(loanamt-gst-pfee));
	}
	
	void loan(int loanamt,int pfee)
	{
		System.out.println("home loan amount is:"+(loanamt-pfee));
	}
	
	public static void main(String[] args) {
		
		//create object
		LoanProcess lp=new LoanProcess();
		lp.loan(500000,2000,3000);
		lp.loan(500000, 3000);
	}
}
